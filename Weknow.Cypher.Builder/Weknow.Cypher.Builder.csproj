<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Product>Weknow Cypher builder</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/weknow-network/Weknow.CypherBuilder</PackageProjectUrl>
    <RepositoryUrl>https://github.com/weknow-network/Weknow.CypherBuilder</RepositoryUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>Weknow.Cypher.Builder.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Weknow.Cypher.Builder.xml" />
    <None Remove="Weknow.CypherBuilder.xml" />
    <None Remove="Weknow.GraphDbCommands.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.ObjectPool" Version="7.0.1" />
    <PackageReference Include="Pluralize.NET" Version="1.0.2" />
    <PackageReference Include="System.Collections.Immutable" Version="7.0.0" />
    <PackageReference Include="Weknow-BasicExtensions" Version="1.0.42" />
    <PackageReference Include="Weknow.Mapping.Contracts" Version="1.0.27" />
  </ItemGroup>

  <ItemGroup>
    <None Include="icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Microsoft.SourceLink.GitHub" Version="1.1.1" />
  </ItemGroup>

</Project>
