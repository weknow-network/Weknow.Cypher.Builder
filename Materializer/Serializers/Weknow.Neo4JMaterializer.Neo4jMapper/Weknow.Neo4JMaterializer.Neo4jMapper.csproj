<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Weknow.Cypher.Builder</PackageId>
    <Authors>Weknow</Authors>
    <Company>Weknow</Company>
    <Product>Weknow Cypher builder</Product>
    <Copyright>Weknow</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/weknow-network/Weknow.Cypher.Builder</PackageProjectUrl>
    <RepositoryUrl>https://github.com/weknow-network/Weknow.Cypher.Builder</RepositoryUrl>
    <Version>2.0.17-alpha</Version>
    <PackageIconUrl />
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>Weknow.Neo4JMaterializer.Neo4jMapper.xml</DocumentationFile>
    <PackageTags>cypher, neo4j, graph, graphdb, orm, csharp, .net, driver, weknow</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Neo4j.Driver" Version="4.0.0" />
  </ItemGroup>

</Project>
